/*********************************************/
/*************** RELATED ITEMS ***************/
/*********************************************/

.click-nav {
    margin: auto;
    padding-right: 5px;
}

.click-nav ul {
    position: relative;
    font-weight: 900;
}

.click-nav ul li {
    position: relative;
    list-style: none;
    cursor: pointer;
}

.click-nav ul li ul {
    position: absolute;
    left: -40;
    right: 0;
    border: 1px solid #9A9A9A;
}

.click-nav ul .clicker {
    position: relative;
    background-color: gainsboro;
    color: black;
    border-left: 1px solid #9A9A9A;
    border-right: 1px solid #9A9A9A;
    border-top: 1px solid #9A9A9A;
    border-bottom: 1px solid #9A9A9A;
}

.click-nav ul .clicker:hover, .click-nav ul .active {
    background: #A6F4FF;
}

.click-nav ul li a {
    transition: background-color 0.2s ease-in-out;
    -webkit-transition: background-color 0.2s ease-in-out;
    -moz-transition: background-color 0.2s ease-in-out;
    display: block;
    padding: 5px 20px 5px 5px;
    background: #FFF;
    color: #333;
    text-decoration: none;
    font-weight: normal;
    font-family: Arial;
}

.click-nav ul li a:hover {
    background-color: #A6F4FF;
    border-right: 1px solid #9A9A9A;
    border-top: 1px solid #9A9A9A;
    border-bottom: 1px solid #9A9A9A;
}

.click-nav ul span img {
    position: relative;
    left: 90px;
}
/*related item dropdown arrow*/
.click-nav ul li a ul img {
    position: relative;
    left: 3px;
    padding-right: 4px;
}
/*related item menu options*/
/* Fallbacks */
.click-nav .no-js ul {
    display: none;
}

.click-nav .no-js:hover ul {
    display: block;
}


