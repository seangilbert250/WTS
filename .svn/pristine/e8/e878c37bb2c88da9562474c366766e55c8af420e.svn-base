/*jquery-ui*/
.ui-widget-overlay {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
}
/*Tabs*/
.ui-tabs .ui-tabs-nav li a {
	padding: 0 3em;
	font-size: 12px;
}

.ui-tabs {
}
.ui-tabs-nav {
	background: transparent;
	border: none;
}
.ui-tabs-panel {
	border: none;
	padding: 2px !important;
	margin: 0px !important;
	text-align: left;
	vertical-align: top;
}
.ui-tabs.ui-widget-content {
	border: none;
}
/*END Tabs*/
/*datepicker*/
.ui-datepicker{
	z-index: 9999 !important;
}

.filterContainer {
	overflow-x: hidden;
	overflow-y: auto;
	margin: 0px;
	padding: 0px;
	font-size: 11px;
	text-align: left;
	border: solid 1px grey;
	height: 400px;
}
	.filterContainer table {
		width: 100%;
		margin: 0px;
		padding: 0px;
	}
	.filterContainer td {
		margin: 0px;
		padding: 0px;
	}

	.filterContainer div {
		font-weight: bold;
		text-align: center;
