/*Calendar*/
.calendar {
	font-size: 12px;
}
	.calendar td {
		padding: 4px;
	}

.calendar_header th:first-child {
	padding: 7px;
	border: solid 1px rgb(33,32,34);
	border-right: none;
	color: #e5e5e5;
	font-weight: normal;
	padding-left: 28px;
}
.calendar_header th:last-child {
	padding: 7px;
	padding-right: 0px;
	border: solid 1px rgb(33,32,34);
	border-left: none;
	color: #e5e5e5;
	font-weight: normal;
}

.calendar_week {
	background-image: url('../../images/page_header_back.gif');
}
	.calendar_week td {
		border-bottom: solid 1px rgb(33,32,34);
		padding: 5px;
		text-align: center;
	}
		.calendar_week td:first-child {
			border-left: solid 1px rgb(33,32,34);
		}
		.calendar_week td:last-child {
			border-right: solid 1px rgb(33,32,34);
		}

.calendar_day {
	width: 30px;
	background: white;
	border-left: solid 1px rgb(33,32,34);
	border-bottom: solid 1px rgb(33,32,34);
	cursor: pointer;
	text-align: center;
}
	.calendar_day:last-child {
		border-right: solid 1px rgb(33,32,34);
	}

.calendar_date {
	width: 30px;
	border-left: solid 1px rgb(33,32,34);
	border-bottom: solid 1px rgb(33,32,34);
	cursor: default;
	text-align: center;
}
	.calendar_date:last-child {
		border-right: solid 1px rgb(33,32,34);
	}


/*Notes*/
.notes {
	width: 100%;
	text-align: right;
	vertical-align: top;
	border: 0px solid gainsboro;
}
.noteThread {
	width: 100%;
	border: 1px solid gainsboro;
	text-align: right;
	vertical-align: top;
	padding: 4px 0px 4px 0px;
}
	.noteThread td {
		padding: 3px;
	}

.noteHeader {
}

.note {
	border: 1px solid gainsboro;
	width: 99%;
}
.noteReplies {
	width: 100%;
	padding: 4px 0px 4px 0px;
}
	.noteReplies table {
	}

.replyButton {
	padding: 1px 2px 1px 2px;
}
