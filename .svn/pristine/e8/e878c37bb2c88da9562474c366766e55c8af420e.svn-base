html, form
{
    overflow:hidden;
}

body {
	margin: 0px;
	padding: 0px;
	font-family: Arial;
	font-size: 12px;
	background: white;
}

ul {
	text-align: left;
	padding: 0;
	margin: 0;
	list-style: none;
	list-style-type: none;
}
	ul li {
		text-align: left;
		list-style: none;
		list-style-type: none;
	}

input, textarea {
	font-family:Arial;
	font-size:12px;
}
input[type="text"], input[type="number"], input[type="date"], input[type="password"], input[type="email"], textarea {
	background-color: #F5F6CE;
}
input[type=button] {
	padding-left: 3px;
	padding-right: 3px;
}

select {
	font-family:Arial;
	font-size:12px;
}

.mainPageContainer {
	width: 100%;
	padding: 0px;
	margin: 0px;
	vertical-align: top;
	text-align: left;
}

.pageContainer {
	width: 100%;
}

.pageContentHeader {
	width: 100%;
	background: url(../../Images/page_header_back.gif);
	border-bottom: 1px solid #d3d3d3;
}

.pageContentInfo {
	width: 100%;
	height: 30px;
	background-color: #D2D2D2;
	border-top: solid 1px white;
}


/*Sidebar*/
.sidebar {
}

.modules {
	font-family: Arial;
	font-weight: normal;
	font-size: 12px;
	text-align: left;
	vertical-align: middle;
}

.moduleImage {
	height: 18px;
}

.moduleText {
	height: 18px;
}

.moduleArrow {
	height: 18px;
}

.moduleRow {
	text-align: left;
	background-image: url(../../Images/Headers/page_header_back.gif);
	cursor: default;
}
	.moduleRow td {
		border-bottom: 1px solid #9A9A9A;
		padding-top: 4px;
		padding-left: 0px;
		padding-bottom: 4px;
		padding-right: 0px;
		text-align: left;
	}
	.moduleRow:hover {
		text-align: left;
		background-image: url(../../Images/Headers/gridheaderblue.gif);
		cursor: default;
	}

.moduleOption {
	font-family: Arial;
	font-weight: normal;
	font-size: 12px;
	text-align: left;
	vertical-align: middle;
	padding: 2px 2px 2px 12px;
}
.moduleOption_Selected {
	background-color: #d7e8fc;
}
/*End Sidebar*/

/*Menus*/
.dropdownMenu {
	margin: 0;
	padding: 0;
	text-align: left;
	font-family: Arial;
	font-size: 12px;
	color: black;
	list-style: none;
	list-style-type: none;
	outline: none;
}
	.dropdownMenu li {
		background-color: transparent;
		text-align: left;
	}

.mainMenu {
	font-family: Arial;
	font-size: 12px;
	width: 100%;
	border-left: 1px solid gray;
	padding: 2px 2px 2px 2px;
	margin: 0px;
	text-align: left;
	background-color: #ECE9D8;
	list-style: none;
}
	.mainMenu li {
		display: inline;
		padding: 2px 2px 2px 2px;
		cursor: default;
	}

.mainMenuHover {
	background-color: #FFFFFF;
}

.mainMenuExpanded {
	border-top: 1px solid gray;
	border-right: 1px solid gray;
	border-left: 1px solid gray;
}

.childMenu {
	font-family: Arial;
	font-size: 12px;
	border-top: 1px solid gray;
	border-right: 1px solid gray;
	border-bottom: 1px solid gray;
	border-left: 1px solid gray;
	padding: 2px 2px 2px 2px;
	margin: 0px;
	text-align: left;
	vertical-align: top;
	background-color: #ECE9D8;
	list-style: none;
}
	.childMenu li {
		padding: 2px 2px 2px 2px;
		vertical-align: middle;
		cursor: default;
	}

.subMenu {
	font-family: Arial;
	font-size: 12px;
	padding: 2px 2px 2px 2px;
	margin: 0px;
	text-align: left;
	vertical-align: top;
	background-color: #ECE9D8;
	list-style: none;
}
	.subMenu li {
		padding: 2px 2px 2px 2px;
		vertical-align: middle;
		cursor: default;
	}
/*End Menubar*/

#sidebar {
	text-align: left;
}

#content {
	overflow: hidden;
	width: auto;
	border-left: solid 1px #9A9A9A;
}

.filterItems {
	background: white;
	border-top: solid 1px gainsboro;
}
	.filterItems tr {
		width: 100%;
		background-image: url(../../images/page_header_back.gif);
		cursor: default;
	}
	.filterItems td {
		border-top: solid 1px white;
		border-left: solid 1px white;
		border-bottom: solid 1px gainsboro;
		padding: 2px;
		padding-left: 5px;
		vertical-align: middle;
	}

/*Popup*/
.PopupWindow_Container {
	border: solid 1px grey;
	background: white;
	margin: 0px;
	padding: 0px;
	/*filter: progid:DXImageTransform.Microsoft.Shadow(Color='#a2a2a2',Direction=0, Strength=13)
            progid:DXImageTransform.Microsoft.Shadow(Color='#a2a2a2',Direction=90, Strength=13)
            progid:DXImageTransform.Microsoft.Shadow(Color='#a2a2a2',Direction=180, Strength=13)
            progid:DXImageTransform.Microsoft.Shadow(Color='#a2a2a2',Direction=270, Strength=13);*/
	-moz-box-shadow: 0px 0px 20px 5px #999;
	-webkit-box-shadow: 0px 0px 20px 5px #999;
	-moz-user-select: -moz-none;
	-khtml-user-select: none;
	-webkit-user-select: none;
	user-select: none;
}

.PopupWindow_Locked {
	border: solid 1px grey;
	margin: 0px;
	padding: 0px;
	-moz-box-shadow: 0px 0px 20px 5px #999;
	-webkit-box-shadow: 0px 0px 20px 5px #999;
	-moz-user-select: -moz-none;
	-khtml-user-select: none;
	-webkit-user-select: none;
	user-select: none;
}
.PopupWindow_Header {
	border-bottom: solid 1px gainsboro;
	margin: 0px;
	padding: 0px;
	background: url('../../images/headers/grey.gif');
	font-weight: bold;
}
	.PopupWindow_Header td {
		height: 20px;
	}

.PopupFooter {
	position: absolute;
	bottom: 0px;
	left: 0px;
	width: 100%;
	text-align: right;
	background: url('../../images/footers/grey.gif');
	margin: 0px;
}
	.PopupFooter table {
		width: 100%;
	}
	.PopupFooter td {
		vertical-align: middle;
		text-align: right;
		height: 20px;
		width: 100%;
		padding: 4px;
		margin: 0px;
	}
/*End Popup*/


.listBoxes {
	border: none;
	border-top-style: none;
	border-bottom-style: none;
	border-left-style: none;
	border-right-style: none;
	border: 0;
	border-style: none;
}

.menuLeft ul {
	list-style: none;
	padding: 0;
	margin: 0;
}

.menuLeft li {
	float: left;
	margin: 0 0.30em;
	cursor: pointer;
}

.menuRight ul {
	list-style: none;
	padding: 0;
	margin: 0;
}

.menuRight li {
	float: right;
	margin: 0 0.30em;
	cursor: pointer;
}

.btn_Link {
	font-family: Arial;
	font-weight: normal;
	font-style: normal;
	text-decoration: underline;
	color: #0078d4;
	margin-left: 10px;
}

/*Grids*/
.metrics {
	text-align: left;
	cursor: default;
}
.metricsHeader {
	font-family: Arial;
	background: url(../../images/headers/gridheaderblue.png);
	border-bottom: 1px solid gainsboro;
}
	.metricsHeader th {
		font-weight: bold;
		text-align: center;
		border-top: 1px solid grey;
		border-right: 1px solid grey;
		border-left: none;
		padding: 4px;
	}

.metricsSubHeader {
	font-family: Arial;
	background-color: InfoBackground;
}
	.metricsSubHeader th {
		font-weight: normal;
		text-align: center;
		border-right: 1px solid grey;
		border-bottom: 1px solid grey;
		border-left: 1px solid gainsboro;
		padding: 4px;
	}

.metricsRow {
	font-family: Arial;
}
	.metricsRow td {
		text-align: center;
		border-top: none;
		border-right: 1px solid grey;
		border-bottom: 1px solid grey;
		border-left: 1px solid gainsboro;
		padding: 4px;
	}
		.metricsRow td a {
		}

.metricsRow_Collab {
	font-family: Arial;
}
	.metricsRow_Collab td {
		text-align: center;
		border: none;
		padding: 4px;
	}
		.metricsRow_Collab td a {
		}

.grid {
	text-align: left;
	cursor: default;
}

.gridHeader {
	font-family: Arial;
	background: url(../../images/headers/gridheaderblue.png);
	border-bottom: 1px solid grey;
	border-top: solid 1px grey;
}
	.gridHeader th {
		text-align: center;
		border-right: 1px solid grey;
		border-left: 1px solid gainsboro;
		border-top: none;
		border-bottom: none;
		padding: 2px;
	}
	.gridHeader td {
		text-align: center;
		border-right: 1px solid grey;
		border-left: 1px solid gainsboro;
		border-top: none;
		border-bottom: none;
		padding: 2px;
	}

.gridBody {
	text-align: left;
}
	.gridBody th {
		text-align: left;
		border-top: none;
		border-right: 1px solid grey;
		border-bottom: none;
		border-left: 1px solid gainsboro;
		padding-top: 3px;
		padding-left: 2px;
		padding-bottom: 3px;
		padding-right: 2px;
	}
	.gridBody td {
		text-align: left;
		border-top: none;
		border-right: 1px solid grey;
		border-bottom: 1px solid grey;
		border-left: none;
		padding: 2px;
	}
		.gridBody td a {
			color: #0000EE;
		}

.gridPager {
	background: #d7e8fc;
	text-align: left;
	padding: 0px;
	margin: 0px;
	border: none;
	background: url(../../images/headers/gridheaderblue.png);
}
	.gridPager td {
		text-align: left;
		padding: 0px;
		margin: 0px;
		border-top: 1px solid grey;
		border-right: none;
		border-bottom: 1px solid grey;
		border-left: none;
	}
		.gridPager td table {
			text-align: left;
			padding: 0px;
			margin: 0px;
		}
			.gridPager td table td {
				border: none;
				padding: 2px;
			}

.selectedRow {
	text-align: left;
	background-color: #ffe8a0;
}
	.selectedRow td {
		border-top: none;
		border-right: 1px solid grey;
		border-bottom: 1px solid grey;
		border-left: none;
		padding: 2px;
		cursor: default;
		text-align: left;
	}
		.selectedRow td a {
			color: #0000EE;
		}

.selectedRow_Header {
	text-align: left;
}
	.selectedRow_Header td {
		border-top: 1px solid grey;
		border-right: 1px solid grey;
		border-bottom: 1px solid grey;
		border-left: none;
		padding: 1px;
		cursor: pointer;
	}


.gridClean {
	text-align: left;
	cursor: default;
}
.gridClean_Header {
	font-family: Arial;
}
	.gridClean_Header th {
		text-align: left;
		border: 0px solid;
		padding: 2px 2px 10px 2px;
	}
	.gridClean_Header td {
		text-align: left;
		border: 0px solid;
		padding: 2px 2px 10px 2px;
	}
.gridClean_Body {
	text-align: left;
	font-family: Arial;
}
	.gridClean_Body th {
		text-align: left;
		border: 0px solid;
		padding: 2px;
	}
	.gridClean_Body td {
		text-align: left;
		border: 0px solid;
		padding: 2px;
	}

.gridSubTotal {
	text-align: left;
	font-family: Arial;
	background-color: #FFFFFF;
}
	.gridSubTotal td {
		text-align: left;
		border-left: 0px solid;
		border-bottom: 0px solid;
		border-right: 0px solid;
		border-top: 1px solid black;
		padding: 4px 2px 2px 2px;
	}


.unreadNews {
	font-weight: bold;
}
.gridHeader_News {
	font-family: Arial;
	background: url(../../images/headers/gridheaderblue.png);
	border-top: solid 1px grey;
	border-bottom: solid 1px grey;
}
	.gridHeader_News th {
		border-top: none;
		border-right: solid 1px grey;
		border-bottom: none;
		border-left: solid 1px gainsboro;
		padding-top: 4px;
		padding-bottom: 4px;
	}
	.gridHeader_News td {
		border-top: none;
		border-right: solid 1px grey;
		border-bottom: none;
		border-left: solid 1px gainsboro;
	}

.gridBody_News {
	text-align: left;
	cursor: pointer;
}
	.gridBody_News td {
		border-left: none;
		border-right: solid 1px grey;
		border-bottom: solid 1px grey;
		border-top: none;
		padding: 2px;
		padding-top: 4px;
		padding-bottom: 4px;
	}
		.gridBody_News td a {
			color: #0000EE;
		}

.selectedRow_News {
	text-align: left;
	background: #ffe8a0;
}
	.selectedRow_News td {
		border-left: none;
		border-right: solid 1px grey;
		border-bottom: solid 1px grey;
		border-top: none;
		padding: 2px;
		padding-top: 4px;
		padding-bottom: 4px;
		cursor: pointer;
	}
		.selectedRow_News td a {
			color: #0000EE;
		}

/*Reports Grids*/
.reportsLinkButton {
	cursor: pointer;
	font-size: 8pt;
	font-family: Arial;
	text-align: center;
	width: 95%;
	background-color: transparent;
	border: none;
}
/*End Reports Grids*/
/*End Grids*/

.notification {
	display: none;
	position: absolute;
	width: 400px;
	left: 0px;
	top: 0px;
	border: solid 1px grey;
	background: white;
	padding: 4px;
	z-index: 100;
}

.myWebsysItems {
	background: white;
	border: solid 1px gray;
	position: absolute;
	width: 140px;
	top: 0px;
	left: 0px;
	display: none;
}

	.myWebsysItems tr {
		width: 100%;
		background-image: url(../../images/page_header_back.gif);
		cursor: pointer;
	}

	.myWebsysItems td {
		border-top: solid 1px white;
		border-left: solid 1px white;
		border-bottom: solid 1px gainsboro;
		padding: 2px;
		padding-left: 5px;
		vertical-align: middle;
	}

/*TreeView*/
.treeView {
	margin-top: -5px;
	padding: 0px;
	font-family: Arial;
	font-size: 12px;
}

.parentTreeNode {
	color: black;
	cursor: default;
	font-weight: bold;
}

.treeNode {
	color: black;
	cursor: default;
}
/*End TreeView*/

/*Attributes*/
.attributes {
}
.attributesRow {
	font-family: Arial;
	font-weight: normal;
	font-size: 12px;
	text-align: left;
	vertical-align: top;
}
	.attributesRow td {
		padding: 2px 2px 2px 2px;
	}
.attributesRequired {
	color: red;
	width: 10px;
	text-align: center;
	vertical-align: middle;
}
.attributesLabel {
	text-align: left;
	vertical-align: middle;
	font-weight: normal;
}
.attributesValue {
	text-align: left;
	vertical-align: middle;
}

/*End Attributes*/

/*Related Items*/
.relatedItems {
	position: absolute;
	display: none;
	top: 0px;
	left: 0px;
	background: #f0f0f0;
	border: solid 1px #979797;
	width: 240px;
	padding-top: 2px;
	padding-bottom: 2px;
	z-index:auto;
	font-family: Segoe UI, Arial;
}

.relatedItemsText {
	border-left: solid 1px #ffffff;
	border-right: solid 1px #f5f5f5;
	vertical-align: middle;
	font-size: 12px;
	color: black;
	padding: 0px;
	margin: 0px;
	padding-left: 4px;
}
	.relatedItemsText div {
		position: relative;
		width: 100%;
		height: 19px;
		padding: 0px;
		margin: 0px;
		padding-top: 3px;
		cursor: default;
		z-index: 2;
	}

.relatedItemsTextSpacer {
	border-left: solid 1px #ffffff;
	border-right: solid 1px #f5f5f5;
	padding-top: 6px;
	padding-bottom: 5px;
}
	.relatedItemsTextSpacer div {
		height: 0px;
		width: 100%;
		border-top: solid 1px #e2e3e3;
		border-bottom: solid 1px #ffffff;
	}

.relatedItemsImage {
	border-left: solid 1px #f5f5f5;
	border-right: solid 1px #e2e3e3;
	vertical-align: middle;
	width: 26px;
}
	.relatedItemsImage div {
		position: relative;
		width: 100%;
		height: 19px;
		padding: 0px;
		margin: 0px;
		padding-top: 3px;
		text-align: center;
		cursor: default;
		z-index: 2;
	}

.relatedItemsImageSpacer {
	border-left: solid 1px #f5f5f5;
	border-right: solid 1px #e2e3e3;
	width: 26px;
}
	.relatedItemsImageSpacer div {
		width: 26px;
		height: 2px;
	}

.relatedItemsSelector {
	width: 100%;
	height: 100%;
	text-align: center;
	position: absolute;
	cursor: default;
	display: none;
	z-index: 0;
}
.relatedItemsSelectorLeft {
	width: 3px;
	height: 22px;
	background: url(../../images/menu/menu_selector_left.png);
}
.relatedItemsSelectorCenter {
	height: 22px;
	background-image: url(../../images/menu/menu_selector_center.png);
}
.relatedItemsSelectorRight {
	width: 3px;
	height: 22px;
	background-image: url(../../images/menu/menu_selector_right.png);
}

/*End Related Items*/

